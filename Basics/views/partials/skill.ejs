<p class="w3-large">
  <b><i class="fa fa-asterisk fa-fw w3-margin-right w3-text-teal"></i>Skills</b>
</p>
<% for(let i = 0; i<skills.length; i++){ %>
  <div class="w3-container">
    <div class=" w3-round-xlarge w3-small"><b><%= skills[i]["title"] %></b></div>
    <div 
      class="w3-container w3-center w3-round-xlarge w3-teal"
      style="width: <%= skills[i]["text"] %>;"
    > 
      <%= skills[i]["text"] %>
    </div>
  </div>
  <%}%>
